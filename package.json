{
  "name": "funcifyr",
  "version": "1.0.0",
  "description": "A functional library used for function composition and creation, and data manipulation.",
  "main": "funcifyr.js",
  "scripts": {
    "test": "mocha tests/funcifyr.spec.js --compilers js:babel-register",
    "watch:test": "npm t -- -w",
    "cover": "nyc npm t"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidrayoussef/funcifyr.git"
  },
  "keywords": [
    "funcifyr"
  ],
  "author": "David Ra Youssef <davidradeveloper@gmail.com> (http://davidra.co/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davidrayoussef/funcify/issues"
  },
  "homepage": "https://github.com/davidrayoussef/funcify#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "ghooks": "^1.3.2",
    "istanbul": "^0.4.5",
    "mocha": "^3.1.2",
    "nyc": "^8.4.0"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run cover"
    }
  },
  "babel": {
